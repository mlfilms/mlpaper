@article{pikin_semi-phenomenological_1999,
	title = {The semi-phenomenological model of antiferroelectricity in chiral smectic liquid crystals {I}. {The} structure of short pitch modes and a thermodynamical approach},
	volume = {26},
	issn = {0267-8292},
	url = {http://dx.doi.org/10.1080/026782999204129},
