@article{cui_anomalous_2004,
	title = {Anomalous {Hydrodynamic} {Interaction} in a {Quasi}-{Two}-{Dimensional} {Suspension}},
	volume = {92},
	url = {http://0-link.aps.org.libraries.colorado.edu/doi/10.1103/PhysRevLett.92.258301},
	doi = {10.1103/PhysRevLett.92.258301},
	abstract = {We study the correlated Brownian motion of micron-sized particles suspended in water and confined between two plates. The hydrodynamic interaction between the particles exhibits three anomalies. (i) The transverse coupling is negative; i.e., particles exert “antidrag” on one another when moving perpendicular to their connecting line. (ii) The interaction decays with interparticle distance r as 1/r2, faster than in unconfined suspensions but slower than near a single wall. (iii) At large distances, the pair interaction is independent of concentration within the experimental accuracy. The confined suspension thus provides an unusual example of long-range, yet essentially pairwise, correlations even at high concentration. These effects are shown to arise from the two-dimensional dipolar form of the flow induced by single-particle motion.},
	number = {25},
	urldate = {2016-03-18},
	journal = {Physical Review Letters},
	author = {Cui, Bianxiao and Diamant, Haim and Lin, Binhua and Rice, Stuart A.},
