@article{salamonczyk_structure_2017,
	title = {Structure of nanoscale-pitch helical phases: blue phase and twist-bend nematic phase resolved by resonant soft {X}-ray scattering},
	volume = {13},
	issn = {1744-6848},
	shorttitle = {Structure of nanoscale-pitch helical phases},
	url = {http://pubs.rsc.org/en/content/articlelanding/2017/sm/c7sm00967d},
	doi = {10.1039/C7SM00967D},
	abstract = {Periodic structures of phases with orientational order of molecules but homogenous electron density distribution: a short pitch cholesteric phase, blue phase and twist-bend nematic phase, were probed by resonant soft X-ray scattering (RSoXS) at the carbon K-edge. The theoretical model shows that in the case of a simple heliconical nematic structure, two resonant signals corresponding to the full and half pitch band should be present, while only the full pitch band is observed experimentally. This suggests that the twist-bend nematic phase has a complex structure with a double-helix built of two interlocked, shifted helices. We confirm that the helical pitch in the twist-bend nematic phase is in a 10 nm range for both the chiral and achiral materials. We also show that the symmetry of the blue phase can be unambiguously determined through a resonant enhancement of the X-ray diffraction signals, by including polarization effects, which are found to be an important indicator in phase structure determination.},
	language = {en},
	number = {38},
	urldate = {2018-07-09},
	journal = {Soft Matter},
	author = {Salamończyk, Mirosław and Vaupotič, Nataša and Pociecha, Damian and Wang, Cheng and Zhu, Chenhui and Gorecka, Ewa},
	month = oct,
	year = {2017},
	pages = {6694--6699}
}

@article{takezoe_antiferroelectric_2010,
	title = {Antiferroelectric liquid crystals: {Interplay} of simplicity and complexity},
	volume = {82},
	shorttitle = {Antiferroelectric liquid crystals},
	url = {https://link.aps.org/doi/10.1103/RevModPhys.82.897},
	doi = {10.1103/RevModPhys.82.897},
	abstract = {This paper reviews nearly 20years of research related to antiferroelectric liquid crystals and gives a short overview of possible applications. “Antiferroelectric liquid crystals” is the common name for smectic liquid crystals formed of chiral elongated molecules that exhibit a number of smectic (Sm) tilted structures with variation of the strong-tilt azimuthal direction from layer to layer (i.e., nonsynclinic structures). The phases have varying crystallographic unit periodicity from a few (SmC∗α), four (SmC∗FI2), three (SmC∗FI1), and two (SmC∗A) smectic layers and all of the phases possess liquidlike order inside the layer. The review describes the discovery of these phases and various methods used for their identification and to determine their structures and their properties. A theoretical description of these systems is also given; one of the models—the discrete phenomenological model—of antiferroelectric liquid crystals is discussed in detail as this model allows for an explanation of phase structures and observed phase sequences under changes of temperature or external fields that is most consistent with experimental results.},
	number = {1},
	urldate = {2018-07-09},
	journal = {Reviews of Modern Physics},
	author = {Takezoe, Hideo and Gorecka, Ewa and Čepič, Mojca},
	month = mar,
	year = {2010},
	pages = {897--937}
}

