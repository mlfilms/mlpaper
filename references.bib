@article{johnson_optical_1999,
	title = {Optical {Reflectivity} and {Ellipsometry} {Studies} of the \${\textbackslash}mathrm\{{Sm}\}-\{{C}\}\_\{{\textbackslash}ensuremath\{{\textbackslash}alpha\}\}{\textasciicircum}\{*\}\$ {Phase}},
