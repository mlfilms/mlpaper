	urldate = {2019-04-11},
	journal = {Physical Review E},
	author = {Bray, A. J. and Puri, Sanjay and Blundell, R. E. and Somoza, A. M.},
	month = apr,
	year = {1993},
	pages = {R2261--R2264}
}

@article{bray_growth_1994,
	title = {Growth laws for phase ordering},
	volume = {49},
	url = {https://link.aps.org/doi/10.1103/PhysRevE.49.R27},
	doi = {10.1103/PhysRevE.49.R27},
	abstract = {We determine the characteristic length scale, L(t), in phase-ordering kinetics for both scalar and vector fields, with either shortor l L(t) consistently by comparing the global rate of energy change to the energy dissipation from the local evolution of the order parameter. We derive growth laws for O(n) and other models, including systems with topological textures.},
