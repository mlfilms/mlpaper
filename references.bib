@article{brown_continuum_2011,
	title = {Continuum simulations of biomembrane dynamics and the importance of hydrodynamic effects},
	volume = {44},
	issn = {1469-8994},
	doi = {10.1017/S0033583511000047},
	abstract = {Traditional particle-based simulation strategies are impractical for the study of lipid bilayers and biological membranes over the longest length and time scales (microns, seconds and longer) relevant to cellular biology. Continuum-based models developed within the frameworks of elasticity theory, fluid dynamics and statistical mechanics provide a framework for studying membrane biophysics over a range of mesoscopic to macroscopic length and time regimes, but the application of such ideas to simulation studies has occurred only relatively recently. We review some of our efforts in this direction with emphasis on the dynamics in model membrane systems. Several examples are presented that highlight the prominent role of hydrodynamics in membrane dynamics and we argue that careful consideration of fluid dynamics is key to understanding membrane biophysics at the cellular scale.},
	language = {eng},
	number = {4},
	journal = {Quarterly Reviews of Biophysics},
	author = {Brown, Frank L. H.},
	month = nov,
	year = {2011},
	pmid = {21729348},
	keywords = {Cell Membrane, Computer Simulation, Diffusion, Elasticity, Hydrodynamics, Kinetics, Lipid Bilayers, Membrane Fluidity, Membrane Microdomains, Models, Biological, Molecular Dynamics Simulation, Time Factors},
	pages = {391--432}
}

