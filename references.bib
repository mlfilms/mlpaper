	keywords = {Polar liquid crystal, antiferroelectric liquid crystal, bent-core liquid crystals, chirality, columnar liquid crystal, discotic liquid crystals, ferroelectric liquid crystal, history, second-harmonic generation, shape asymmetry, switching current},
	pages = {1--17}
}

