	url = {http://onlinelibrary.wiley.com/doi/10.1002/adfm.201302295/abstract},
	doi = {10.1002/adfm.201302295},
	abstract = {A new series of azobenzene containing bent-core molecules incorporating 4-cyanoresorcinol as the central core unit exhibiting cybotactic nematic, rectangular, columnar, and different types of tilted smectic (SmC) phases are synthesized. The mesophase behavior and phase structures are characterized in bulk and freely suspended films using a variety of experimental techniques. Depending on the chain length and temperature a series of different mesophases is observed in these compounds, ranging from cybotactic nematic via paraelectric SmC phases, polarization randomized SmCsPR phases to ferroelectric and antiferroelectric SmC phases, associated with increasing size and correlation length of the polar domains. Spontaneous formation of chiral domains is observed in the paraelectric SmC and the SmCsPR phases and discussed in relation with superstructural chirality, bend elastic constants, and surface effects.},
	language = {en},
	number = {12},
	urldate = {2018-01-10},
	journal = {Advanced Functional Materials},
	author = {Alaasar, Mohammed and Prehm, Marko and May, Kathrin and Eremin, Alexey and Tschierske, Carsten},
	month = mar,
	year = {2014},
	keywords = {bent-core mesogens, chirality, ferroelectricity, liquid crystals, phase transitions},
	pages = {1703--1717}
}

