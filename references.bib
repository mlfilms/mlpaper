	volume = {113},
	url = {https://link.aps.org/doi/10.1103/PhysRevLett.113.128304},
	doi = {10.1103/PhysRevLett.113.128304},
	abstract = {We study experimentally and theoretically the hydrodynamic interaction of pairs of circular inclusions in two-dimensional, fluid smectic membranes suspended in air. By analyzing their Brownian motion, we find that the radial mutual mobilities of identical inclusions are independent of their size but that the angular coupling becomes strongly size dependent when their radius exceeds a characteristic hydrodynamic length. These observations are described well for arbitrary inclusion separations by a model that generalizes the Levine-MacKintosh theory of point-force response functions and uses a boundary-element approach to calculate the mobility matrix for inclusions of finite extent.},
	number = {12},
	urldate = {2018-06-07},
	journal = {Physical Review Letters},
